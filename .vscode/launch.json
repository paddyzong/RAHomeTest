{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Django Tests in core.tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Backend/RATypeInfer/manage.py",
            "args": [
                "test",
                "core.tests", 
                "--verbosity",
                "2"
            ],
            "django": true,
            "console": "integratedTerminal"
        }
    ]
}
